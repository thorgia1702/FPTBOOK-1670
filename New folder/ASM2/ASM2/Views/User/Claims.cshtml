@model List<Post>
@{
    ViewData["Title"] = "View User Claims";
}
<h1 class="text-center text-uppercase">View User Claims</h1>

<br />

@if (User.Identity.IsAuthenticated)
{
    <table class="table table-striped table-bordered">
        @foreach (var claim in User.Claims)
        {
            <tr>
                <td>@claim.Type</td>
                <td>@claim.Value</td>
                <td>@claim.Issuer</td>
            </tr>
        }
    </table>
}